{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "functions": {
    "dist/app-porfolio/server/main.js": {}
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/app-porfolio/browser/server/main.js"
    }
  ]
}